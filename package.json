{
  "name": "@treely/strapi-slices",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Tree.ly GmbH",
  "description": "@treely/strapi-slices is a open source library maintained by Tree.ly.",
  "repository": {
    "type": "git",
    "url": "https://github.com/treely/strapi-slices.git"
  },
  "bugs": {
    "url": "https://github.com/treely/strapi-slices/issues"
  },
  "homepage": "https://tree.ly",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/strapi-slices.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/test/setupTests.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!**/src/test/**",
      "!**/src/index.ts",
      "!**/*.de.ts",
      "!**/*.stories.ts",
      "!**/*.stories.tsx",
      "!**/node_modules/**",
      "!**/.next/**",
      "!**/coverage/**",
      "!**/.storybook/**",
      "!**/storybook-static/**"
    ]
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "next": "^14.0.3",
    "react": ">=18",
    "react-intl": ">=6"
  },
  "engines": {
    "node": ">=16 <=18"
  },
  "size-limit": [
    {
      "path": "dist/strapi-slices.cjs.production.min.js",
      "limit": "2 MB"
    },
    {
      "path": "dist/strapi-slices.esm.js",
      "limit": "600 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addons": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/nextjs": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/vite-react": "^2.0.1",
    "@types/mapbox-gl": "^2.7.19",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "dts-cli": "^2.0.3",
    "esbuild": "^0.19.8",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "mq-polyfill": "^1.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^22.0.8",
    "size-limit": "^11.0.0",
    "storybook": "^7.5.3",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.15",
    "adblock-detect-react": "^1.1.0",
    "boemly": "^5.7.6",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.5",
    "mapbox-gl": "^2.15.0"
  }
}

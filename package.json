{
  "name": "@treely/strapi-slices",
  "version": "0.0.0-development",
  "license": "MIT",
  "author": "Tree.ly FlexCo",
  "description": "@treely/strapi-slices is a open source library maintained by Tree.ly.",
  "repository": {
    "type": "git",
    "url": "https://github.com/treely/strapi-slices.git"
  },
  "bugs": {
    "url": "https://github.com/treely/strapi-slices/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "homepage": "https://tree.ly",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit",
    "build": "tsx esbuild.config.ts",
    "lint": "eslint src",
    "size": "size-limit",
    "start": "tsx esbuild.config.ts --watch",
    "test": "jest --passWithNoTests",
    "prepare": "tsx esbuild.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/test/setupTests.ts"
    ],
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-intl|@formatjs|intl-messageformat)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^lodash-es$": "lodash"
    },
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!**/src/test/**",
      "!**/src/index.ts",
      "!**/*.de.ts",
      "!**/*.stories.ts",
      "!**/*.stories.tsx",
      "!**/node_modules/**",
      "!**/.next/**",
      "!**/coverage/**",
      "!**/.storybook/**",
      "!**/storybook-static/**"
    ]
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "next": "^15 || ^16",
    "react": "^18 || ^19",
    "react-intl": ">=6"
  },
  "engines": {
    "node": ">=22 <=24"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "620 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-links": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/nextjs": "^10.1.10",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@tsconfig/vite-react": "^2.0.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/mapbox-gl": "^2.7.19",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitest/mocker": "^4.0.15",
    "esbuild": "^0.27.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^10.1.10",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-intl": "^8.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "semantic-release": "^25.0.1",
    "size-limit": "^11.0.0",
    "storybook": "^10.1.10",
    "ts-jest": "^29.4.6",
    "tsx": "^4.19.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.8",
    "@types/geojson": "^7946.0.16",
    "adblock-detect-react": "^1.1.0",
    "axios": "^1.7.2",
    "axios-cache-interceptor": "^1.5.3",
    "boemly": "^10.0.1",
    "embla-carousel-auto-scroll": "^8.5.1",
    "embla-carousel-react": "^8.5.1",
    "formik": "^2.4.5",
    "framer-motion": "^12.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "mapbox-gl": "^2.15.0",
    "qs": "^6.13.1",
    "schema-dts": "^1.1.5",
    "swr": "^2.3.2"
  },
  "msw": {
    "workerDirectory": [
      "public",
      ".storybook/public"
    ]
  }
}
